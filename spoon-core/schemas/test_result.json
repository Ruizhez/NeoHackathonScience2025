{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TestResult",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the test result"
    },
    "date": {
      "type": "string",
      "format": "date",
      "description": "Date when the test was conducted"
    }
  },
  "required": ["name", "date"],
  "additionalProperties": false
}